generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Feed {
  id          String   @id @default(uuid())
  accessToken String
  botId       String?
  databaseId  String
  properties  String   // JSON stringified configuration
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
