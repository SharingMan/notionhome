generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Feed {
  id            String   @id @default(uuid())
  accessToken   String
  botId         String?
  displayName   String?
  databaseId    String
  properties    String   // JSON stringified configuration
  ownerKey      String?
  ownerUserId   String?
  ownerUserName String?
  workspaceId   String?
  workspaceName String?
  workspaceIcon String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@index([ownerKey])
  @@index([workspaceId])
}
